{
    "manifest_version": 2,
    "name": "Tioreo",
    "description": "Tools for your browser",
    "version": "1.0.0",
    "author": "exl1pse",
    "browser_action": {
        "default_title": "Tioreo",
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["jquery-3.6.0.min.js", "cleaner.js"]
        }
      ],
    "permissions": ["history", "browsingData", "downloads"]
}